   "source": [
    "import numpy as np\n",
    "\n",
    "M,P,L = 100,2,1\n",
    "F = np.random.uniform(0,1,size=[M,P])**0.5-0.5\n",
    "U, S, Wh = np.linalg.svd(F, full_matrices=True)\n",
    "\n",
    "W_L = Wh[:L,:].T\n",
    "\n",
    "\n",
    "G = F.dot(W_L)\n",
    "\n",
    "\n",
    "print(W_L,F,G)\n",
    "\n"
   ]
